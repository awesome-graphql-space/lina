    "history": "^4.7.2",
    "jsonwebtoken": "^8.2.2",
    "lint-staged": "^7.1.2",
    "mini-css-extract-plugin": "^0.4.0",
    "node-fetch": "^2.1.2",
    "pg": "^7.4.3",
    "react": "^16.4.0",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.0",
    "react-fade-in": "^0.1.6",
    "react-helmet": "^5.2.0",
