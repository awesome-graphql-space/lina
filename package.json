{
  "name": "linaframework",
  "version": "0.0.1",
  "description": "linaframework",
  "author": "Rex Raphael",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "razzle:start": "razzle start",
    "razzle:build": "razzle build",
    "razzle:test": "razzle test --env=jsdom",
    "razzle:prod": "NODE_ENV=production node build/server.js",
    "build": "npm run build:frontend && npm run build:backend && npm run docker:prod",
    "build:publish": "npm run build:frontend && npm run build:backend && npx release-it -n --no-npm.publish && npm run docker:prod:publish",
    "build:frontend": "cross-env NODE_ENV=production node scripts/build.js",
    "build:backend": "webpack  --env.NODE_ENV=production --env.watchFiles=false --config scripts/api-webpack.config.js",
    "start:frontend": "cross-env NODE_ENV=development node scripts/start.js",
    "start:backend": "npx ts-node -r tsconfig-paths/register src/main",
    "start": "npm run start:frontend & npm run start:backend",
    "server": "npx pm2 start --name=node-api dist/server.js",
    "docker:prod": "node scripts/docker-build.js",
    "docker:prod:publish": "node scripts/docker-build.js --publish",
    "test": "cross-env API_DB_USERNAME=postgres API_DB_PASSWORD=password API_DB_NAME=postgres node scripts/test.js --env=jsdom --config=jest.backend.config.js",
    "test:frontend": "node scripts/test.js --env=jsdom --config=jest.frontend.config.js",
    "test:update": "yarn test --updateSnapshot",
    "lint": "eslint ./src/client",
    "generate:docs": "npx typedoc --out docs/ src/ --mode file --module commonjs --target es6",
    "generate:defintions": "npx dts-generator --name node-advanced-api --project ./ --out node-advanced-api.d.ts",
    "generate-all": "npm run generate:defintions && npm run generate:docs",
    "webpack": "webpack --env.NODE_ENV=development --env.watchFiles=true --config scripts/api-webpack.config.js"
  },
  "lint-staged": {
    "*.{js,css}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint src --fix"
    ]
  },
  "moduleRoots": [
    "src/client",
    "src/server",
    "src",
    "node_modules"
  ],
  "dependencies": {
    "@material-ui/core": "^1.4.0",
    "@material-ui/icons": "^1.1.0",
